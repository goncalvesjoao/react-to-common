{
  "name": "my_react_component",
  "version": "1.0.0",
  "description": "my commonJS react component",
  "repository": {
    "type": "git",
    "url": "https://github.com/"
  },
  "main": "dist/index.js",
  "scripts": {
    "start": "npm run spa",
    "build": "rm -rdf ./dist && ./node_modules/.bin/babel ./src/ --out-dir ./dist/",
    "quick-test": "./node_modules/.bin/mocha ./test/**/*_spec.js",
    "test": "rm -rdf ./coverage && npm run lint && node ./node_modules/.bin/babel-istanbul cover ./node_modules/.bin/_mocha ./test/**/*_spec.js",
    "lint": "./node_modules/.bin/eslint ./src/ ./test/",
    "spa": "rm -rdf ./spa/tmp && ./node_modules/.bin/gulp --gulpfile ./spa/config/gulpfile.js"
  },
  "license": "MIT",
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-core": "^5.8.25",
    "babel-eslint": "^4.1.2",
    "babel-istanbul": "^0.3.20",
    "babel-loader": "^5.3.2",
    "chai": "^3.2.0",
    "eslint": "^1.4.3",
    "eslint-plugin-react": "^3.3.2",
    "jsdom": "^6.5.1",
    "mocha": "^2.3.2",
    "react-test-tree": "^0.3.1",
    "spa": "file:./spa"
  },
  "dependencies": {
    "aja": "^0.4.0",
    "react": "^0.13.3"
  }
}
